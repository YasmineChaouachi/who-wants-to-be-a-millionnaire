package model;

import java.awt.Color;

/* cet enum specifi les couleurs de l'application. */

public enum ColorProject {

	COLOR_BLUE(new Color(45,153,196)),
	COLOR_GREY(new Color(105,97,88));

	private Color col;
	
	ColorProject(Color color){
		this.col=color;
	}

	/*cette methode retourne la couleur de l'enumeration.	 */
	public Color getCol() {
		return col;
	}
	
	
}
